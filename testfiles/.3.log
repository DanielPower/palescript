local player = {}:
	vec = {}:
		x = 40
		y = 30
		function draw():
			love.graphics.rectangle('fill', player.vec.x, player.vec.y, 32, 32)
	speed = 12
function player.draw():
	love.graphics.rectangle('fill', player.vec.x, player.vec.y, 32, 32)
function love.update(dt):
	if love.keyboard.isDown("a"):
		player.vec.x -= player.speed
	elseif love.keyboard.isDown("d"):
		player.vec.x += player.speed
function love.draw():
	player.draw()
